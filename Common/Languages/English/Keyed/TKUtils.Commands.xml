    <TKUtils.Commands.Description.ReviveAllShortcut>A colony wide resurrector mech serum.</TKUtils.Commands.Description.ReviveAllShortcut>
    <TKUtils.Commands.Usage.ReviveAllShortcut>reviveall</TKUtils.Commands.Usage.ReviveAllShortcut>
    <TKUtils.Commands.UserLevel.ReviveAllShortcut>Anyone</TKUtils.Commands.UserLevel.ReviveAllShortcut>
    
    <TKUtils.Commands.Description.ReviveMeShortcut>A personal resurrector mech serum.</TKUtils.Commands.Description.ReviveMeShortcut>
    <TKUtils.Commands.Usage.ReviveMeShortcut>reviveme</TKUtils.Commands.Usage.ReviveMeShortcut>
    <TKUtils.Commands.UserLevel.ReviveMeShortcut>Anyone</TKUtils.Commands.UserLevel.ReviveMeShortcut>
    
    <TKUtils.Commands.Description.HealMeShortcut>A personal healer mech serum.</TKUtils.Commands.Description.HealMeShortcut>
