    <TKUtils.Commands.Description.MyPawnStory>Shows your pawn's bio.</TKUtils.Commands.Description.MyPawnStory>
    <TKUtils.Commands.Usage.MyPawnStory>mypawnstory</TKUtils.Commands.Usage.MyPawnStory>
    <TKUtils.Commands.UserLevel.MyPawnStory>Anyone</TKUtils.Commands.UserLevel.MyPawnStory>
    
